{
  "name": "back",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/data-source.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./src/data-source.ts migration:generate ./src/migrations/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./src/migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./src/data-source.ts migration:revert",
    "test": "jest --verbose --coverage --coverageReporters=html"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "helmet": "^7.1.0",
    "jest": "^29.7.0",
    "joy": "^0.1.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "next": "14.2.5",
    "next-auth": "^4.24.7",
    "next-swagger-doc": "^0.4.0",
    "nextjs-cors": "^2.2.0",
    "pg": "^8.12.0",
    "react": "^18",
    "react-dom": "^18",
    "react-helmet": "^6.1.0",
    "reflect-metadata": "^0.2.2",
    "sanitizer": "^0.1.3",
    "typeorm": "^0.3.20",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/chai": "^4.3.17",
    "@types/cors": "^2.8.17",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-helmet": "^6.1.11",
    "@types/sanitizer": "^0.0.31",
    "chai": "^5.1.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "mocha": "^10.7.3",
    "sinon": "^18.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
